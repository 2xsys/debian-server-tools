{
    "name": "szepeviktor/phpstan-wordpress",
    "description": "stubs-generator depends on php-parser v3: giacocorsiglia/stubs-generator:^0.5.0",
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpstan/phpstan-shim": "^0.11.5",
        "giacocorsiglia/wordpress-stubs": "^4.9"
    },
    "autoload-dev": {
        "psr-4": {
            "PHPStan\\WordPress\\": "tests/phpstan/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "classmap-authoritative": true
    }
}
